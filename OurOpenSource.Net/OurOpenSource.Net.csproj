  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>D:\Work\Project\OurOpenSource\source\OurOpenSource\OurOpenSource.Net\OurOpenSource.Net.xml</DocumentationFile>
    <GenerateSerializationAssemblies>Auto</GenerateSerializationAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="SslClient.cs" />
    <Compile Remove="SslServer.cs" />
    <Compile Remove="SslStreamHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="SslClient.cs" />
    <None Include="SslServer.cs" />
    <None Include="SslStreamHelper.cs" />
  </ItemGroup>

</Project>
